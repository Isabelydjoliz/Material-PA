algoritmo "dollarpreal"
// Diciplina : PA
// Professora : Cínta
// Turma: A
// Data : 02/03/2023
// Nome : Isabely
var
rela,cota,dol: real



//ENTRADA
inicio
escreval ("Valor em dollar: ")
leia (dol)

escreval ("Valor da cotação: ")
leia (cota)



//PROCESSAMENTO
rela <- (cota*dol)


// Saída
escreval ("O valor em real é: ", rela )


fimalgoritmo
