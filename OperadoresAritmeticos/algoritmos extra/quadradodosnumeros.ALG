algoritmo "quadradodosnumeros"
// Autor :Isabely
// Data : 30/03/2023
// Professora: cintia
var
n1, n2, n3, n4, resul1, resul2, resul3, resul4, soma: real

inicio

//estrada

escreval ("Digite o primeiro número:")
leia (n1)
escreval ("Digite o segundo número:")
leia (n2)
escreval ("Digite o terceiro número")
leia (n3)
escreval ("Digite o quarto número")
leia (n4)

//processamento

resul1 <- n1*n1
resul2 <- n2*n2
resul3 <- n3*n3
resul4 <- n4*n4
soma <- resul1+resul2+resul3+resul4

// saída
escreval("o resultado do quadrado do primeiro número será: ", resul1)
escreval("o resultado do quadrado do segundo número será: ", resul2)
escreval("o resultado do quadrado do terceiro número será: ", resul3)
escreval("o resultado do quadrado do quarto número será: ", resul4)
fimalgoritmo